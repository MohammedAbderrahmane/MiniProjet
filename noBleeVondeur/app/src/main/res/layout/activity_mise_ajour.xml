<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MiseAJourActivity">

    <Button
        android:id="@+id/miseajour_ajouter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ajouter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/miseajour_modifier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="modifier"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/miseajour_ajouter"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:id="@+id/miseajour_frame_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/miseajour_modifier" />

</androidx.constraintlayout.widget.ConstraintLayout>